        "styled-components": "5.0.0"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ]
    },
    "publishConfig": {
        "ignore": [
            "!dist/",
            "!/node_modules/"
        ]
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true,
        "moduleResolution": "node"
    }
