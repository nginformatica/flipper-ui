        "eslint-plugin-storybook": "0.6.7",
        "fs-extra": "7.0.0",
        "gh-pages": "2.0.0",
        "jest": "24.9.0",
        "node-fetch": "2.6.1",
        "node-polyfill-webpack-plugin": "2.0.1",
        "prettier": "2.8.0",
        "react": "16.14.0",
        "react-dom": "16.14.0",
