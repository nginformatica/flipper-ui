        "@material-ui/lab": "4.0.0-alpha.54",
        "@material-ui/pickers": "3.2.6",
        "date-fns": "2.4.1",
        "faker": "5.5.3",
        "material-table": "1.69.3",
        "material-ui-chip-input": "2.0.0-beta.2",
        "ramda": "0.25.0",
        "react-number-format": "4.4.1",
        "uuid": "9.0.0"
    },
