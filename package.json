    "date-fns": "2.4.1",
    "material-table": "1.59.0",
    "material-ui-chip-input": "2.0.0-beta.2",
    "ramda": "0.25.0",
    "react-number-format": "4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.0",
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
